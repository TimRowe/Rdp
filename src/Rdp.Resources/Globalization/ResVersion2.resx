  <data name="ReadCard1" xml:space="preserve">
    <value>&lt;object id="MIF" width="0" height="0" classid="CLSID:7BEC3845-30E9-11D6-BB52-00D0096A436E"&gt;
        &lt;param name="COM" /&gt;
        &lt;param name="SECTOR" value="1" /&gt;
        &lt;param name="BLOCK" value="0" /&gt;
    &lt;/object&gt;
    &lt;script type="text/vbscript"&gt;
    DIM s
    s = 1
    // readeropen()
    Sub RC()
        DIM a, b, s2, str
        s2 = s
        a = ""
        b = ""
        call MIF.MIFAREREADSERIAL()
        s = MIF.CAPTION
        //check if the same card is placed on the reader
        If s = s2 Then
            ID = settimeout("RC()",300)
            Exit Sub
        End If
	  
        Call MIF.MIFARELEDRED()
        Call MIF.MIFAREREADCARD()
        a=MIF.CAPTION
	  
        F_ReadCard.Staff_Code.VALUE = ""
      
        If a &lt;&gt; "R-1" and b &lt;&gt; "R-1" then
            F_ReadCard.Staff_Code.VALUE = ucase(trim(mid(a,1,16)))
            CALL MIF.MIFARELEDGREEN()
            CALL MIF.MIFAREBEEP()
            Call readerclose()
        End If
	  
        ID = SetTimeOut("RC()",300)
    End Sub

    Sub readerclose()
        cleartimeout ID
        CALL mif.mifareledyellow()
        CALL MIF.MIFARECLOSECOMM()
    End Sub
	
    Sub readeropen()
        Call MIF.MIFAREAUTOFIND()
        MIF.COM=MIF.CAPTION
        //MIF.COM=1
        Call MIF.MIFAREOPENCOMM()
  
        //MIF.CAPTION = 5
        //call MIF.MIFAREEXBEEP()
        ID = SetTimeOut("RC()",300)
    End Sub
    Sub BT_ReadCard_OnClick
     Call readeropen()
    End Sub
    &lt;/script&gt;</value>
